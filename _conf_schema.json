{
    "intercept": {
        "description": "拦截模块配置",
        "type": "object",
        "hint": "",
        "items": {
            "reread_block":{
                "description": "拦截复读消息",
                "type": "bool",
                "hint": "打开后，若Bot当前要发送的消息和前5次发送的消息有重复的，则被拦截",
                "default": true
            },
            "block_error": {
                "description": "拦截报错",
                "type": "bool",
                "hint": "打开后，若Bot将要发送的消息是报错消息，则被拦截（但bot管理员触发的不拦截，方便排查）",
                "default": true
            },
            "error_words": {
                "description": "报错拦截所用关键词",
                "hint": "Bot发的消息中，包含列表中任一关键字时，消息将被拦截",
                "type": "list",
                "default": [
                    "请求失败",
                    "出现异常"
                ]
            },
            "block_ai": {
                "description": "人机拦截",
                "type": "bool",
                "hint": "打开后，若Bot将要发送的消息太过于人机，则被拦截",
                "default": true
            },
            "ai_words": {
                "description": "人机拦截所用关键词",
                "hint": "Bot发的消息中，包含列表中任一关键字时，消息将被拦截",
                "type": "list",
                "default": [
                    "抱歉",
                    "您好",
                    "请稍后",
                    "隐私政策",
                    "作为AI助手",
                    "作为一个AI",
                    "我理解您的感受",
                    "让我来为您解释",
                    "根据相关法规",
                    "很抱歉给您带来不便",
                    "感谢您的理解",
                    "感谢您的耐心等待",
                    "为您带来的不便深表歉意",
                    "如有需要请随时联系",
                    "如果您还有其他问题",
                    "我将尽力为您提供帮助",
                    "希望我的回答对您有所帮助",
                    "该问题需要进一步核实",
                    "请您理解并配合",
                    "暂时无法提供相关信息",
                    "不支持该请求",
                    "不在支持范围内",
                    "该内容暂不可用",
                    "出于安全原因",
                    "出于合规要求",
                    "为保障您的账户安全",
                    "为了保护您的隐私",
                    "根据平台规则",
                    "根据系统检测",
                    "由系统自动生成"
                ]
            }
        }
    },
    "clean": {
        "description": "文本清理配置",
        "type": "object",
        "hint": "",
        "items": {
            "text_threshold": {
                "description": "文本清理长度阈值",
                "type": "int",
                "hint": "多少个字符以下时执行文本清理，可以防止对长文本误清理",
                "default": 150
            },
            "format_at": {
                "description": "摘掉括号式At",
                "hint": "些模型喜欢开头附加 [At:xxx] 或 [At：xxx], Deepseek模型抽风时经常出现。",
                "type": "bool",
                "default": true
            },
            "fake_at": {
                "description": "摘掉假@",
                "hint": "一些模型喜欢发假@，即字符串式的“@昵称”",
                "type": "bool",
                "default": true
            },
            "emoji": {
                "description": "清理emoji",
                "type": "bool",
                "hint": "清理文本中的emoji，对喜欢发emoji的模型有奇效（比如智谱）",
                "default": true
            },
            "lead": {
                "type": "list",
                "description": "去除指定句首字符",
                "hint": "对某些喜欢输出前缀的模型有奇效，注意只去除句首字符",
                "default": [
                    "print",
                    ":",
                    "："
                ]
            },
            "tail": {
                "type": "list",
                "description": "去除指定句尾字符",
                "hint": "对某些喜欢输出句号的模型有奇效，注意只去除句尾字符",
                "default": [
                    "。",
                    "."
                ]
            },
            "punctuation": {
                "type": "string",
                "description": "整体清洗的特殊符号",
                "hint": "填写正则表达式，例如：[#%~], 注意是整句清洗",
                "default": "[#%~]"
            }
        }
    },
    "reat": {
        "description": "引用与@配置",
        "type": "object",
        "hint": "",
        "items": {
            "reply_switch": {
                "description": "引用回复",
                "type": "bool",
                "hint": "当bot要回复的消息不是群里的最新消息时（被顶上去了），引用要回复的消息。 请提前关闭Astrbot自动的Reply功能",
                "default": 0.1
            },
            "at_prob": {
                "description": "@发送者概率",
                "type": "float",
                "hint": "当消息中包含文本时，按概率@发送者。, 请提前关闭Astrbot自动的At功能",
                "default": 0.001
            },
            "enable_at_str": {
                "description": "换用字符串@发送者",
                "type": "bool",
                "hint": "Astrbot在拼接At与Plain组件时，中间的空格会消失，不介意的话可以换用字符串At来解决此问题",
                "default": true
            }
        }
    }
}